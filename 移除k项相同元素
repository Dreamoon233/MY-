
class Solution {
    public int removeDuplicates(int[] nums) {
        int n=nums.length;
        if (n<=2)
        {
            return n;
        }
        int slow=0;
        for(int x:nums)
        {
            if(slow<2 ||nums[slow-2]!=x)
            {
                nums[slow++]=x;
            }
        }
        return slow;
    }
